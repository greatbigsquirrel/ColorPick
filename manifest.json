{
   "manifest_version": 2,
   "description": "__MSG_extDescription__",
   "name": "__MSG_extName__",
   "browser_action": {
      "default_icon": {
        "19": "icon19.png"/*,
        "38": "icon38.png"*/
      },
      "default_title": "Color Picker",
      "default_popup": "popup.html"
    },
   "content_scripts": [ {
      "js": [ "Cr_min.js", "colorpick.user.js" ],
      "run_at": "document_start",
      "matches": [ "*://*/*" ]
   },{
      "js": [ "installkey.user.js" ],
      "matches": [ "http://vidsbee.com/OrderComplete.php*", "https://vidsbee.com/OrderComplete.php" ],
      "run_at": "document_end"
   } ],
   "background": {
      "scripts": ["background.js"]
   },
   "options_page": "options.html",
   "icons": { "16": "icon16.png",
           "32": "icon32.png",
           "48": "icon48.png",
           "64": "icon64.png",
          "128": "icon128.png" },
   "web_accessible_resources": [
     "crosshair.png",
     "close.png"
   ],
   "permissions": [
      "tabs", "webRequest", "webNavigation", "*://*/*"
   ],
   "content_security_policy": "script-src 'self' https://cdn.extensionanalytics.com; object-src 'self'",
   "default_locale": "en",
   "version": "0.0.1.42"
}
